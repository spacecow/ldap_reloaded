<table id="report">
  <tr>
    <th><%= sortable 'accounts.username', t(:username) %></th>
    <th><%= sortable 'memberships.path', t(:path) %></th>
    <th><%= sortable 'groups.gid', t(:gid) %></th>
    <th><%= sortable 'groups.gidname', t(:gidname) %></th>
    <th><%= sortable 'day_count', pl(:day) %></th>
    <th><%= pl(:month) %></th>
    <th><%= sortable 'day_of_registration' %></th>
  </tr>
  <% mstats.each do |mstat| %>
    <%= render 'months/tablemonth', mstat:mstat %>
  <% end %>
</table>
